<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.std.rsShop.login.dao.LoginMapper">

    <select id="findByLoginId" resultType="com.std.rsShop.login.vo.UserInfo">
        select
        *
        from
        member
        where
        login_id = #{loginId};
    </select>
</mapper>
